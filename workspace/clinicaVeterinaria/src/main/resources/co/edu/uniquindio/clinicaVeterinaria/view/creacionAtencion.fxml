<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.clinicaVeterinaria.controllers.CreacionAtencionController">
   <children>
      <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label alignment="TOP_CENTER" prefHeight="17.0" prefWidth="301.0" text="Fecha" />
            <Label alignment="TOP_CENTER" prefHeight="17.0" prefWidth="309.0" text="Veterinario" GridPane.rowIndex="1" />
            <Label fx:id="lblFechaAtencion" alignment="TOP_CENTER" prefHeight="17.0" prefWidth="300.0" text="Label" GridPane.columnIndex="1" />
            <Label fx:id="lblVeterinario" alignment="TOP_CENTER" prefHeight="17.0" prefWidth="301.0" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <TableView prefHeight="217.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="columnaNombreMascota" prefWidth="75.0" text="Nombre" />
          <TableColumn fx:id="columnaEdadMascota" prefWidth="75.0" text="Edad" />
            <TableColumn fx:id="columnaSexoMascota" prefWidth="75.0" text="Sexo" />
            <TableColumn fx:id="columnaTipoMascota" prefWidth="75.0" text="Tipo" />
            <TableColumn fx:id="columaRazaMascota" prefWidth="75.0" text="Raza" />
        </columns>
      </TableView>
      <Button fx:id="btnCrearAtencion" mnemonicParsing="false" onMouseClicked="#crearAtencion" text="Crear" />
   </children>
</VBox>
